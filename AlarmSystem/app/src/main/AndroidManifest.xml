<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.pubnub.examples.pubnubExample10"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk
            android:minSdkVersion="10"
            android:targetSdkVersion="17"/>

    <application android:allowBackup="true"
                 android:icon="@drawable/pubnub_large"
                 android:label="@string/app_name"
                 android:theme="@style/AppTheme">

        <activity android:name="com.pubnub.examples.pubnubExample10.Accessibility"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".AlarmPanel"
            android:label="Alarm Panel">
        </activity>
        <activity android:name=".AlarmManagerActivity"
            android:label="Intruder detected">
            <intent-filter>
                <action
                    android:name="android.permission.VIBRATE"/>
            </intent-filter>
        </activity>
		<receiver
             android:name=".GcmBroadcastReceiver"
             android:permission="com.google.android.c2dm.intent.SEND"
             >
            <intent-filter>
               <action
                   android:name="com.google.android.c2dm.permission.RECEIVE"/>
            </intent-filter>

           </receiver>
        <!--<receiver-->
            <!--android:name=".AlarmManagerActivity"-->
            <!--android:permission="com.google.android.c2dm.permission.RECEIVE"-->
          <!-->-->
        <!--</receiver>-->
        <service android:name=".GcmIntentService" />
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />
    

</manifest>